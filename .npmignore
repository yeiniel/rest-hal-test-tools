
# Ignore source files
/src/*.ts
# But do not ignore declaration files
!/src/*.d.ts

# Ignore documentation generated by the command `npm run typedoc`
/docs/

# Ignore build configuration files and project settings files
/tsconfig.json
/tslint.json
/.travis.yml
/.editorconfig

# Ignore any IDE specific file
/.idea/
/.vscode/

# Ignore project tests
/src/*.spec.js

# Ignore coverage report files
/.nyc_output/
/coverage/
